{
  "name": "notification-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint \"{src,app,libs,test}/**/*.{tsx,js,ts}\" --fix --max-warnings 0",
    "postinstall": "patch-package",
    "prebuild": "rimraf android && rimraf ios && npx expo prebuild",
    "android:build": "eas build --platform android",
    "ios:build": "eas build --platform ios",
    "ios:submit": "eas submit --platform ios"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.40.1",
    "axios": "^1.7.2",
    "expo": "^51.0.11",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.7",
    "expo-router": "~3.5.15",
    "expo-status-bar": "~1.12.1",
    "lodash-es": "^4.17.21",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "private": true
}
